{
    "name": "indigophp/cart",
    "description": "Cart abstraction layer",
    "license": "MIT",
    "keywords": ["shopping", "cart"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Cart\\Stub\\": "stub/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
