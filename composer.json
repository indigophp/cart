{
    "name": "indigophp/cart",
    "description": "Cart abstraction layer",
    "license": "MIT",
    "keywords": ["shopping", "cart"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "2.1.*@dev,>=2.1.0-RC2",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Cart\\Stub\\": "stub/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
